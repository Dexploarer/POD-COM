{
  "name": "@pod-protocol/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for PoD Protocol (Prompt or Die) AI Agent Communication Protocol",
  "type": "module",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w", 
    "test": "bun test",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "bun run build"
  },
  "packageManager": "bun@1.0.0",
  "keywords": [
    "solana",
    "ai-agents",
    "agent-communication",
    "blockchain-protocol",
    "web3-sdk",
    "decentralized-messaging",
    "prompt-or-die",
    "pod-protocol",
    "typescript",
    "anchor"
  ],
  "author": "PoD Protocol Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dexploarer/PoD-Protocol.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/Dexploarer/PoD-Protocol/issues"
  },
  "homepage": "https://github.com/Dexploarer/PoD-Protocol#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/web3.js": "^1.95.4",
    "@solana/wallet-adapter-base": "^0.9.23"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.18.0",
    "rollup": "^4.28.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.95.0"
  }
}